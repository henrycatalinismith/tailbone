__lua__

mode = 'attract'
score = 0
paused = false

frame = 0
timeouts = {}
intervals = {}
eventloop = {}
script = {}

trex = {}
ollie_cooloff = false

cacti = {}
cars = {}

tricks = {
  'none',
  'push',
  'pop',
  'ollie',
  'charge',
  'grab',
}

is_attack = {
  none = false,
  push = false,
  pop = false,
  ollie = false,
  charge = true,
  grab = true,
}

trick = 'none'
charge = 0
