    self.offset.x + 128, 0,
    1
  )

  for i = flr(self.offset.x),flr(self.offset.x) + 128 do
    if i % 4 < 1 then
      x1 = i
      x2 = x1 + 0
      y1 = -10 - (i % 30) * 10
      y2 = y1 + 0
      circfill(
        x1, y1,
        0,
        6
      )
    end
  end
end

