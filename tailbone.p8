sprites = {
  roll_1 = { 0, 0, 16, 16, 0, false },
  roll_1 = { 0, 0, 16, 16, 0, false },
  push_1 = { 16, 0, 16, 16, 0, false },
  push_2 = { 32, 0, 16, 16, 0, false },
  push_3 = { 48, 0, 16, 16, 0, false },
  push_4 = { 64, 0, 16, 16, 0, false },
  push_5 = { 80, 0, 16, 16, 0, false },
  push_6 = { 96, 0, 16, 16, 0, false },

  ollie_1 = { 0, 16, 16, 16, 0, false },
  grab_1 = { 16, 16, 16, 16, 0, false },
  grab_2 = { 32, 16, 16, 16, 0, false },
  dead_1 = { 48, 16, 16, 16, 0, false },
  ollie_2 = { 64, 16, 16, 16, 0, false },

  cactus = { 0, 32, 8, 16, 0, false },
  plant_1 = { 8, 32, 8, 8, 0, false },
  plant_2 = { 16, 32, 8, 8, 0, false },
  plant_3 = { 24, 32, 8, 8, 0, false },
  plant_4 = { 32, 32, 8, 8, 0, false },
  car_right_1 = { 8, 40, 24, 12, 1, false },
  car_left_1 = { 8, 40, 24, 12, 1, true },
  cable_1 = { 32, 32, 32, 16, 0, false },
  tower_1 = { 64, 32, 16, 16, 0, false },
  tower_2 = { 80, 32, 16, 16, 0, false },
  tower_3 = { 96, 32, 16, 16, 0, false },
  road = { 0, 53, 16, 17, 0, false },
  logo = { 0, 64, 96, 32, 0 },
}

actions = {
  cactus = function()
    if mode == 'play' and paused == false then
      nextx = trex.offset[1] + 120
      add(cacti, {
        offset = { nextx, 0 },
        size = { 6, 16 },
      })
      after(2^7, 'next')
    end
  end,

  charge = function()
    trick = 'charge'
    charge = 0
    trex.vector[2] = trex.vector[2] / 2
    sfx(3)
    after(2^5, 'charge_limit')
  end,

  charge_limit = function()
    if trick == 'charge' then
      trick = 'grab'
      trex.vector[2] = charge / 4
    end
  end,

  cooloff = function()
    ollie_cooloff = false
  end,

  destroy_cactus = function()
    trick = 'air'
    trex.offset[2] = (
      cacti[1].offset[2] -
      cacti[1].size[2]
    )
    trex.vector[2] = -4
  end,

  destroy_car = function()
    trick = 'air'
    trex.offset[2] = trex.offset[2] - 4
    trex.vector[2] = -4
  end,

  gc = function()
    for i,cactus in pairs(cacti) do
      if cactus.offset[1] < foreground.offset[1] then
        del(cacti, i)
      end
    end

    for i,car in pairs(cars) do
      d = foreground.offset[1] + 64 - car.offset[1]
      if d > 256 then
        del(cars, i)
      end
    end
  end,

  gameover = function()
    if paused == false and trex.alive == true then
      paused = true
      sfx(2)
      trick = 'none'
      trex.alive = false
    end
  end,

  grab = function()
    trick = 'grab'
    trex.vector[2] = charge / 4
  end,

  land = function()
    if trick == 'grab' then
      cooloff_time = 2^4
    else
      cooloff_time = 2^4
    end

    if trex.alive then
      sfx(1)
      trick = 'push'
    end
    ollie_cooloff = true
    trex.offset[2] = 0
    trex.vector[2] = 0
    after(cooloff_time, 'cooloff')
  end,

  next = function()
    action = script[1]
    del(script, action)
    add(eventloop, action)

    if flr(rnd(2)) == 1 then
      add(script, 'cactus')
    else
      add(script, 'overtake')
      add(script, 'ram')
    end
  end,

  pop = function()
    if ollie_cooloff then
      return
    end
    trick = 'pop'
    trex.vector[2] = -3.2
    sfx(0)
  end,

  ollie = function()
    trick = 'ollie'
  end,

  overtake = function()
    if mode == 'play' and paused == false then
      x = trex.offset[1] - 64
      s = trex.vector[1] * 1.7
      add(cars, {
        offset = { x, 6 },
        size = { 21, 8 },
        vector = { s, 0 },
      })
      after(2^8, 'next')
    end
  end,

  play = function()
    if mode == 'play' then
      return true
    end

    printh('play!!')
    mode = 'play'
    trex.offset[1] = foreground.offset[1] + trex.size[1]

    add(script, 'cactus')
    add(eventloop, 'next')
  end,

  ram = function()
    if mode == 'play' and paused == false then
      x = trex.offset[1] + 128
      s = trex.vector[1] * -1
      add(cars, {
        offset = { x, 3 },
        size = { 21, 8 },
        vector = { s, 0 },
      })
      after(2^6, 'next')
    end
  end,

  release = function()
    trick = 'air'
  end,

  reset = function()
    score = 0
    ollie_cooloff = false
    trick = 'push'
    mode = 'attract'
    paused = false

    for k,v in pairs(timeouts) do
      del(timeouts, k)
    end
    for k,v in pairs(script) do
      del(script, k)
    end
    for k,v in pairs(cacti) do
      del(cacti, k)
    end
    for k,v in pairs(cars) do
      del(cars, k)
    end

    cars = {}
    cacti = {}
    trex.alive = true
    trex.trick = 'push'
    trex.offset = { 10, 0 }
    trex.vector = { 1.5, 0 }
    trex.size = { 16, 16 }
    foreground.offset = { -10, -96 }
    foreground.vector = { trex.vector[1], 0 }
    nearground.offset = { -10, -92 }
    nearground.vector = { 1, 0 }
    cityscape.offset = { -10, -82 }
    cityscape.vector = { 1, 0 }
    sky.offset = { -10, -96 }
    sky.vector = { 0.5, 0 }
    cls()
  end,

}

hitboxes = {
  cactus = function(c)
    return {
      offset = {
        c.offset[1] + 2,
        c.offset[2] - c.size[2],
      },
      size = {
        c.size[1] - 4,
        c.size[2],
      }
    }
  end,

  car = function(c)
    return {
      offset = {
        c.offset[1] + 1,
        c.offset[2] - c.size[2] + 2,
      },
      size = {
        c.size[1],
        c.size[2] - 3,
      },
    }
  end,

  trex = function()
    if trick == 'grab' then
      return {
        offset = {
          trex.offset[1] + 2,
          trex.offset[2] - trex.size[2] + 4,
        },
        size = { 12, 6 }
      }
    end

    return {
      offset = {
        trex.offset[1] + 2,
        trex.offset[2] - trex.size[2] + 8,
      },
      size = { 8, 6 }
    }

  end,
}


