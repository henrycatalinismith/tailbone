-- point -----------------------

point = {}

function point:new(x, y)
  self.__index = self
  return setmetatable({
    x = x,
    y = y,
  }, self)
end

function point.__add(a, b)
  return point:new(
    a.x + b.x,
    a.y + b.y
  )
end

--------------------------------
-- vector ----------------------

vector = {}

function vector:new(x, y)
  self.__index = self
  return setmetatable({
    x = x,
    y = y,
  }, self)
end

function vector.__add(a, b)
  return vector:new(
    a.x + b.x,
    a.y + b.y
  )
end


--------------------------------
-- box -------------------------

box = {}

function box:new(x, y, w, h)
  self.__index = self
  return setmetatable({
    x = x,
    y = y,
    w = w,
    h = h,
  }, self)
end

function box:p1()
  return {
    x = self.x,
    y = self.y
  }
end

function box:p2()
  return {
    x = self.x + self.w,
    y = self.y + self.h
  }
end

function box:over(b)
  return self.y <= b.y - self.h
     and self.x == b.x
end

function box:above(b)
  return self.y <= b.y - self.h
end

function box:intersects(b)
   -- left
  if self.x <= b.x - self.w then
    return false
  end

  -- right
  if self.x >= b.x + b.w then
    return false
  end

  -- above
  if self.y <= b.y - self.h then
    return false
  end

  -- below
  if self.y >= b.y + b.h then
    return false
  end

  return true
end

